<h1>Users#index</h1>
<%= link_to("Add new user",{:action=>"new"}) %>
<ul><% @users.each do |u| %>
<li><%= u.username%>
  <%= link_to("show",{:action=>"show",:id=>u.id}) %>
  <%= link_to("edit",{:action=>"edit",:id=>u.id}) %>
  <%= link_to("delete",{:action=>"delete",:id=>u.id}) %></li>
<%end%>
</ul>

